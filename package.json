{
  "name": "flixf-lex-movies-app",
  "version": "1.3.0",
  "description": "FlixFlex movies app",
  "author": "Idris Chaalel <chaalelidris8@gmail.com>",
  "license": "MIT-0",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "babel src -d dist --copy-files",
    "start": "cross-env NODE_ENV=production dotenv -e .env node dist/index.js",
    "dev": "cross-env NODE_ENV=development dotenv -e .env babel-watch -w package.json -w src src/index.js",
    "test": "cross-env NODE_ENV=test dotenv -e .env jest --verbose",
    "test:watch": "cross-env NODE_ENV=test dotenv -e .env jest --watch",
    "lint": "eslint src"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.0.0",
    "morgan": "^1.10.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.11",
    "@babel/node": "^7.22.10",
    "@babel/preset-env": "^7.22.10",
    "babel-watch": "^7.8.1",
    "@types/jest": "^29.5.4",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  }
}
